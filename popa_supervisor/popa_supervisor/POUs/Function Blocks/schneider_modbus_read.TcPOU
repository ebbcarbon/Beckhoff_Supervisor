		total_power_factor_convert(array_pos_0 := arrayReadDataReg_2[50], array_pos_1 := arrayReadDataReg_2[51], output => total_power_factor);
		system_frequency_convert(array_pos_0 := arrayReadDataReg_2[10], array_pos_1 := arrayReadDataReg_2[11], output => system_frequency);
		avg_current_capacity_convert(array_pos_0 := arrayReadDataReg_2[42], array_pos_1 := arrayReadDataReg_2[43], output => avg_current_capacity);
		power_factor_lagging_convert(array_pos_0 := arrayReadDataReg_2[72], array_pos_1 := arrayReadDataReg_2[73], output => power_factor_lagging);
		power_factor_leading_convert(array_pos_0 := arrayReadDataReg_2[74], array_pos_1 := arrayReadDataReg_2[75], output => power_factor_leading);
(*	3:
