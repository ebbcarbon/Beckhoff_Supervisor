			sIPAddr:= ip_addr, 
			nTCPPort:= tcp_port, 
			nUnitID:= unit_ID, 
			nQuantity:= read_regQty, 
			nMBAddr:= reg_address, 
			cbLength:= SIZEOF(arrayReadDataReg_2), 
			pDestAddr:= ADR(arrayReadDataReg_2), 
			bExecute:= read_frequency.Q, 
			tTimeout:= timeout, 
